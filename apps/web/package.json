{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap && pagefind --site .next/server/app --output-path public/_pagefind",
    "clean": "rm -rf node_modules/ .turbo/ .next/",
    "check-types": "tsc --noEmit",
    "dev": "next dev --turbopack",
    "format": "prettier --write . --ignore-path ./.gitignore",
    "lint": "eslint .",
    "start": "next start",
    "update": "pnpm update --latest"
  },
  "dependencies": {
    "@workspace/ui": "workspace:*",
    "lucide-react": "0.479.0",
    "next": "^15.1.7",
    "nextra": "^4.2.16",
    "nextra-theme-docs": "^4.2.16",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@workspace/eslint": "workspace:*",
    "@workspace/tailwind": "workspace:*",
    "@workspace/typescript": "workspace:*",
    "next-sitemap": "^4.2.3",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.8"
  }
}
